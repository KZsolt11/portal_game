<!ELEMENT Maze (Items?,Walls?)>
<!ELEMENT Items (Hole*,ZPM*,Scale*, Box*)>
<!ELEMENT Hole (#PCDATA)>
<!ELEMENT ZPM (#PCDATA)>
<!ELEMENT Scale (#PCDATA)>
<!ELEMENT Box (#PCDATA)>
<!ELEMENT Walls (Wall*)>
<!ELEMENT Wall (#PCDATA)>

<!ATTLIST Maze	height CDATA #REQUIRED>
<!ATTLIST Maze	width CDATA #REQUIRED>
<!ATTLIST Maze	name CDATA #REQUIRED>

<!ATTLIST Hole	posX CDATA #REQUIRED>
<!ATTLIST Hole	posY CDATA #REQUIRED>

<!ATTLIST ZPM	posX CDATA #REQUIRED>
<!ATTLIST ZPM	posY CDATA #REQUIRED>

<!ATTLIST Box	posX CDATA #REQUIRED>
<!ATTLIST Box	posY CDATA #REQUIRED>
<!ATTLIST Box	weight CDATA "1">


<!ATTLIST Scale	posX CDATA #REQUIRED>
<!ATTLIST Scale	posY CDATA #REQUIRED>
<!ATTLIST Scale	id CDATA #REQUIRED>

<!ATTLIST Wall	posX CDATA #REQUIRED>
<!ATTLIST Wall posY CDATA #REQUIRED>
<!ATTLIST Wall	isSpecial CDATA "false">
<!ATTLIST Wall hasDoor CDATA "false">
<!ATTLIST Wall scaleID CDATA #IMPLIED>
<!ATTLIST Wall orientation CDATA #REQUIRED>